{
  "name": "govuk-prototype-kit",
  "description": "Rapidly create HTML prototypes of GOV.UK services",
  "version": "12.1.0",
  "private": true,
  "engines": {
    "node": ">=12.0.0 <17.0.0"
  },
  "scripts": {
    "start": "node start.js",
    "lint": "standard",
    "rapidtest": "jest --bail",
    "cypress:open": "cypress open",
    "test:integration": "start-server-and-test 3000 'cypress run'",
    "test:integration:edge": "start-server-and-test 3000 'cypress run --browser edge'",
    "test:integration:chrome": "start-server-and-test 3000 'cypress run --browser chrome'",
    "test:integration:electron": "start-server-and-test 3000 'cypress run --browser electron'",
    "test": "gulp generate-assets && jest && npm run lint"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "^1.4.2",
    "acorn": "^8.5.0",
    "ansi-colors": "^4.1.3",
    "body-parser": "^1.14.1",
    "browser-sync": "^2.11.1",
    "client-sessions": "^0.8.0",
    "cookie-parser": "^1.4.6",
    "cross-spawn": "^7.0.2",
    "del": "^6.1.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "fancy-log": "^1.3.3",
    "govuk_frontend_toolkit": "^7.5.0",
    "govuk_template_jinja": "^0.24.1",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^4.1.0",
    "gray-matter": "^4.0.3",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.2.1",
    "gulp-sass": "^5.1.0",
    "inquirer": "^8.2.0",
    "lodash": "^4.17.21",
    "luxon": "^2.4.0",
    "marked": "^4.0.16",
    "notifications-node-client": "^5.1.0",
    "nunjucks": "^3.2.1",
    "portscanner": "^2.1.1",
    "require-dir": "^1.0.0",
    "sass": "^1.52.1",
    "sync-request": "^6.0.0",
    "universal-analytics": "^0.4.16",
    "uuid": "^8.3.2"
  },
  "optionalDependencies": {
    "cypress": "^9.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "glob": "^8.0.3",
    "jest": "^27.3.1",
    "standard": "^14.3.3",
    "start-server-and-test": "^1.14.0",
    "supertest": "^6.1.6",
    "wait-on": "^6.0.1"
  },
  "standard": {
    "plugins": [
      "cypress"
    ],
    "env": {
      "cypress/globals": true
    }
  },
  "jest": {
    "testRunner": "jest-jasmine2"
  },
  "main": "gulpfile.js",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/demo-updated-moj-template-project.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ministryofjustice/demo-updated-moj-template-project/issues"
  },
  "homepage": "https://github.com/ministryofjustice/demo-updated-moj-template-project#readme"
}
